<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- sample-test-environment.xml -->
<!-- Used for configuration of test environment -->
<sample-test-environment>
	<atf>
		<web>
			<base-url>http://store.agiletestingframework.com:8080/candystore/
			</base-url>
		</web>

		<desired-capabilities>
			<browser-name>firefox</browser-name>
			<browser-version>31.0</browser-version>
			<browser-download-path>C:\Program Files (x86)\Mozilla Firefox\31.0\firefox.exe
			</browser-download-path>
			<platform></platform>
			<takes-screenshot>true</takes-screenshot>
			<handles-alerts></handles-alerts>
			<css-selectors-enabled></css-selectors-enabled>
			<javascript-enabled></javascript-enabled>
			<database-enabled></database-enabled>
			<location-context-enabled></location-context-enabled>
			<application-cache-enabled></application-cache-enabled>
			<browser-connection-enabled></browser-connection-enabled>
			<web-storage-enabled></web-storage-enabled>
			<accept-ssl-certs></accept-ssl-certs>
			<rotatable></rotatable>
			<native-events></native-events>
			<proxy></proxy>
			<unexpected-alert-behavior></unexpected-alert-behavior>
			<element-scroll-behavior></element-scroll-behavior>

			<json-proxy-type></json-proxy-type>
			<json-proxy-auto-config-url></json-proxy-auto-config-url>
			<json-proxy></json-proxy>
			<json-socks-username></json-socks-username>
			<json-socks-password></json-socks-password>
			<json-no-proxy></json-no-proxy>
			<json-logging-component></json-logging-component>

			<remote-webdriver-remote-quiet-exceptions></remote-webdriver-remote-quiet-exceptions>

			<grid-use>false</grid-use>
			<grid-url>http://localhost:4443/wd/hub</grid-url>
			<!-- Using Saucelabs -->
			<!-- <grid-url>http://stephen-rufle-atf:b349a801-f092-40f7-8857-46d849064db3@ondemand.saucelabs.com:80/wd/hub</grid-url> -->
			<grid-selenium-protocol></grid-selenium-protocol>
			<grid-max-instances></grid-max-instances>
			<phantomjs>
				<executable-path-property></executable-path-property>
				<page-settings-prefix></page-settings-prefix>
				<page-customheaders-prefix></page-customheaders-prefix>
				<cli-args></cli-args>
				<ghostdriver-path-property></ghostdriver-path-property>
				<ghostdriver-cli-args></ghostdriver-cli-args>
			</phantomjs>
			<opera>
				<binary></binary>
				<guess-binary-path></guess-binary-path>
				<no-restart></no-restart>
				<product></product>
				<no-quit></no-quit>
				<autostart></autostart>
				<display></display>
				<idle></idle>
				<profile></profile>
				<launcher></launcher>
				<port></port>
				<host></host>
				<arguments></arguments>
				<logging-file></logging-file>
				<logging-level></logging-level>
			</opera>
			<chrome>
				<proxy></proxy>
				<chrome-options>
					<args></args>
					<binary></binary>
					<extentions></extentions>
				</chrome-options>
			</chrome>

			<firefox>
				<profile-dir-and-filename></profile-dir-and-filename>
				<logging-prefs></logging-prefs>
				<binary></binary>
				<rc-mode></rc-mode>
				<rc-capture-network-traffic></rc-capture-network-traffic>
				<rc-add-custom-request-header></rc-add-custom-request-header>
				<rc-trust-all-ssl-cert></rc-trust-all-ssl-cert>
				<webdriver-accept-untrusted-certs></webdriver-accept-untrusted-certs>
				<webdriver-assume-untrusted-issuer></webdriver-assume-untrusted-issuer>
				<webdriver-log-driver></webdriver-log-driver>
				<webdriver-log-file></webdriver-log-file>
				<webdriver-log-file></webdriver-log-file>
				<webdriver-load-strategy></webdriver-load-strategy>
				<webdriver-port></webdriver-port>
			</firefox>

			<ie>
				<ignore-protected-mode-settings></ignore-protected-mode-settings>
				<ignore-zoom-setting></ignore-zoom-setting>
				<initial-browser-url></initial-browser-url>
				<enable-persistent-hover></enable-persistent-hover>
				<enable-element-cache-cleanup></enable-element-cache-cleanup>
				<require-window-focus></require-window-focus>
				<browser-attach-timeout></browser-attach-timeout>
				<force-create-process-api></force-create-process-api>
				<browser-cmd-line-switches></browser-cmd-line-switches>
				<use-per-process-proxy></use-per-process-proxy>
				<ensure-clean-session></ensure-clean-session>
				<log-file></log-file>
				<log-level></log-level>
				<host></host>
				<extract-path></extract-path>
				<silent></silent>
				<set-proxy-by-server></set-proxy-by-server>
				<rc-mode></rc-mode>
				<rc-kill-processes-by-name></rc-kill-processes-by-name>
				<rc-honor-system-proxy></rc-honor-system-proxy>
				<rc-ensure-clean-session></rc-ensure-clean-session>
			</ie>
			<safari>
				<use-options></use-options>
				<data-dir></data-dir>
				<driver-extension></driver-extension>
				<skip-extension-installation></skip-extension-installation>
				<port></port>
				<use-clean-session></use-clean-session>
				<rc-mode></rc-mode>
				<rc-honor-system-proxy></rc-honor-system-proxy>
			</safari>
		</desired-capabilities>

		<reporting>
			<!-- report file locations, switches to turn different reporting on\off -->
			<use-jserrorcollector-with-firefox>false</use-jserrorcollector-with-firefox>
			<buffered-image-save-location>C:\SavedFiles\</buffered-image-save-location>
			<buffered-image-save-format>PNG</buffered-image-save-format>
		</reporting>

		<localization>
			<locale>en-US</locale>
			<resource-path>src/test/resources</resource-path>
		</localization>

		<database>
			<!-- primary datasource -->
			<db-driver>org.h2.Driver</db-driver>
			<db-user>sa</db-user>
			<db-password></db-password>
			<db-url>jdbc:h2:tcp://192.168.1.49:9123/mem:devDb</db-url>
		</database>

		<mobile>
			<common>
			    <automationName>Appium</automationName>
				<appiumVersion></appiumVersion>
				<appiumStartNodeServer>true</appiumStartNodeServer>
				<appiumNodeServerCommandLine>cmd</appiumNodeServerCommandLine>
				<appiumNodeServerArguments>/c,
					C:/Progra~2/appium/Appium/Node.exe,
					C:/Progra~2/appium/Appium/node_modules/appium/bin/appium.js,
					--address, 127.0.0.1,
					--port, 4723,
					--full-reset
				</appiumNodeServerArguments>
				<appiumNodeServerProcessName>Node.exe</appiumNodeServerProcessName>
				<gridUrl>http://127.0.0.1:4723/wd/hub
				</gridUrl>

				<platformName>IOS</platformName>
				<platformVersion>5.1</platformVersion>
				<deviceName>04ea252d124877ce</deviceName>
				<deviceType></deviceType> <!-- tablet -->
				<browserName>Chrome</browserName>
				<newCommandTimeout></newCommandTimeout>
				<app></app>
				<autoLaunch></autoLaunch>
				<language></language>
				<locale></locale>
				<udid></udid>
				<orientation></orientation>
				<autoWebview></autoWebview>
				<noReset></noReset>
				<fullReset></fullReset>
			</common>
			<android>
				<appActivity></appActivity>
				<appPackage></appPackage>
				<appWaitActivity></appWaitActivity>
				<appWaitPackage></appWaitPackage>
				<deviceReadyTimeout></deviceReadyTimeout>
				<androidCoverage></androidCoverage>
				<enablePerformanceLogging></enablePerformanceLogging>
				<androidDeviceReadyTimeout></androidDeviceReadyTimeout>
				<androidDeviceSocket></androidDeviceSocket>
				<avd></avd>
				<avdLaunchTimeout></avdLaunchTimeout>
				<avdReadyTimeout></avdReadyTimeout>
				<avdArgs></avdArgs>
				<useKeystore></useKeystore>
				<keystorePath></keystorePath>
				<keystorePassword></keystorePassword>
				<keyAlias></keyAlias>
				<keyPassword></keyPassword>
				<chromedriverExecutable></chromedriverExecutable>
				<autoWebviewTimeout></autoWebviewTimeout>
				<intentAction></intentAction>
				<intentCategory></intentCategory>
				<intentFlags></intentFlags>
				<optionalIntentArguments></optionalIntentArguments>
				<stopAppOnReset></stopAppOnReset>
				<unicodeKeyboard></unicodeKeyboard>
				<resetKeyboard></resetKeyboard>
				<noSign></noSign>
				<ignoreUnimportantViews></ignoreUnimportantViews>
			</android>
			<ios>
				<calendarFormat></calendarFormat>
				<bundleId></bundleId>
				<launchTimeout></launchTimeout>
				<locationServicesEnabled></locationServicesEnabled>
				<locationServicesAuthorized></locationServicesAuthorized>
				<autoAcceptAlerts></autoAcceptAlerts>
				<autoDismissAlerts></autoDismissAlerts>
				<nativeInstrumentsLib></nativeInstrumentsLib>
				<nativeWebTap></nativeWebTap>
				<safariInitialUrl></safariInitialUrl>
				<safariAllowPopups></safariAllowPopups>
				<safariIgnoreFraudWarning></safariIgnoreFraudWarning>
				<safariOpenLinksInBackground></safariOpenLinksInBackground>
				<keepKeyChains></keepKeyChains>
				<localizableStringsDir></localizableStringsDir>
				<processArguments></processArguments>
				<interKeyDelay></interKeyDelay>
				<showIOSLog></showIOSLog>
				<sendKeyStrategy></sendKeyStrategy>
				<screenshotWaitTimeout></screenshotWaitTimeout>
				<waitForAppScript></waitForAppScript>
			</ios>
		</mobile>
		<other>
			<image-library>
				<windows>${resource-path}/${app-path}/image</windows>
				<linux>${resource-path}/${app-path}/image</linux>
				<macosx>${resource-path}/${app-path}/image</macosx>
			</image-library>
		</other>
	</atf>

</sample-test-environment>